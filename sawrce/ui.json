{
  "design_system": {
    "name": "Midnight Emerald",
    "version": "1.0.0",
    "philosophy": "Dark luxury with surgical precision. Every pixel serves a purpose.",
    "principles": [
      "Depth through subtlety, not complexity",
      "Motion should feel physical, not decorative",
      "Typography is the primary interface element",
      "Color is used sparingly for maximum impact"
    ]
  },

  "colors": {
    "background": {
      "primary": "#0A0A0B",
      "secondary": "#121214",
      "tertiary": "#1A1A1D",
      "elevated": "#18181B"
    },
    "surface": {
      "card": "rgba(24, 24, 27, 0.6)",
      "card_border": "rgba(63, 63, 70, 0.4)",
      "input": "rgba(9, 9, 11, 0.8)",
      "hover": "rgba(255, 255, 255, 0.03)"
    },
    "accent": {
      "primary": "#10B981",
      "primary_hover": "#34D399",
      "primary_glow": "rgba(16, 185, 129, 0.2)",
      "secondary": "#8B5CF6",
      "danger": "#F43F5E",
      "warning": "#F59E0B",
      "info": "#3B82F6"
    },
    "text": {
      "primary": "#FAFAFA",
      "secondary": "#A1A1AA",
      "tertiary": "#71717A",
      "disabled": "#52525B",
      "inverse": "#09090B"
    },
    "feedback": {
      "success": "#10B981",
      "error": "#F43F5E",
      "error_subtle": "rgba(244, 63, 94, 0.1)",
      "success_subtle": "rgba(16, 185, 129, 0.1)"
    }
  },

  "typography": {
    "font_family": {
      "sans": "Inter, system-ui, -apple-system, sans-serif",
      "mono": "JetBrains Mono, monospace",
      "display": "Cal Sans, Inter, sans-serif"
    },
    "scale": {
      "hero": { "size": "48px", "weight": "800", "tracking": "-0.04em", "line_height": "1.1" },
      "h1": { "size": "36px", "weight": "700", "tracking": "-0.03em", "line_height": "1.2" },
      "h2": { "size": "28px", "weight": "700", "tracking": "-0.02em", "line_height": "1.2" },
      "h3": { "size": "22px", "weight": "600", "tracking": "-0.01em", "line_height": "1.3" },
      "h4": { "size": "18px", "weight": "600", "tracking": "-0.01em", "line_height": "1.3" },
      "body": { "size": "16px", "weight": "400", "tracking": "0", "line_height": "1.6" },
      "body_small": { "size": "14px", "weight": "400", "tracking": "0", "line_height": "1.5" },
      "caption": { "size": "12px", "weight": "500", "tracking": "0.05em", "line_height": "1.4" },
      "label": { "size": "10px", "weight": "600", "tracking": "0.2em", "line_height": "1.2", "transform": "uppercase" }
    }
  },

  "spacing": {
    "base_unit": "4px",
    "scale": {
      "xs": "4px",
      "sm": "8px",
      "md": "16px",
      "lg": "24px",
      "xl": "32px",
      "2xl": "48px",
      "3xl": "64px",
      "4xl": "96px"
    },
    "component": {
      "button_padding": "16px 24px",
      "input_padding": "14px 18px",
      "card_padding": "24px",
      "section_gap": "48px"
    }
  },

  "border_radius": {
    "sm": "6px",
    "md": "10px",
    "lg": "16px",
    "xl": "24px",
    "2xl": "32px",
    "full": "9999px"
  },

  "shadows": {
    "sm": "0 1px 2px 0 rgba(0, 0, 0, 0.3)",
    "md": "0 4px 6px -1px rgba(0, 0, 0, 0.4), 0 2px 4px -1px rgba(0, 0, 0, 0.2)",
    "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.5), 0 4px 6px -2px rgba(0, 0, 0, 0.3)",
    "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.6), 0 10px 10px -5px rgba(0, 0, 0, 0.4)",
    "glow": {
      "primary": "0 0 20px rgba(16, 185, 129, 0.15)",
      "primary_hover": "0 0 30px rgba(16, 185, 129, 0.25)",
      "error": "0 0 20px rgba(244, 63, 94, 0.15)"
    },
    "inner": "inset 0 2px 4px 0 rgba(0, 0, 0, 0.3)"
  },

  "effects": {
    "backdrop_blur": {
      "sm": "blur(8px)",
      "md": "blur(16px)",
      "lg": "blur(24px)",
      "xl": "blur(32px)"
    },
    "glassmorphism": {
      "background": "rgba(24, 24, 27, 0.6)",
      "border": "1px solid rgba(255, 255, 255, 0.08)",
      "backdrop_filter": "blur(20px)"
    },
    "gradients": {
      "surface": "linear-gradient(180deg, rgba(255,255,255,0.03) 0%, rgba(255,255,255,0) 100%)",
      "glow_top": "radial-gradient(circle at 50% 0%, rgba(16, 185, 129, 0.08) 0%, transparent 50%)",
      "glow_corner": "radial-gradient(circle at 100% 0%, rgba(16, 185, 129, 0.05) 0%, transparent 40%)"
    }
  },

  "motion": {
    "duration": {
      "instant": "0ms",
      "fast": "150ms",
      "normal": "250ms",
      "slow": "350ms",
      "slower": "500ms"
    },
    "easing": {
      "default": "cubic-bezier(0.4, 0, 0.2, 1)",
      "in": "cubic-bezier(0.4, 0, 1, 1)",
      "out": "cubic-bezier(0, 0, 0.2, 1)",
      "bounce": "cubic-bezier(0.34, 1.56, 0.64, 1)",
      "smooth": "cubic-bezier(0.25, 0.1, 0.25, 1)"
    },
    "transitions": {
      "button": "all 200ms cubic-bezier(0.4, 0, 0.2, 1)",
      "card": "all 300ms cubic-bezier(0.4, 0, 0.2, 1)",
      "modal": "all 400ms cubic-bezier(0.25, 0.1, 0.25, 1)",
      "color": "color 150ms ease, background-color 150ms ease"
    }
  },

  "components": {
    "button": {
      "primary": {
        "background": "#10B981",
        "color": "#0A0A0B",
        "border": "none",
        "border_radius": "16px",
        "padding": "16px 24px",
        "font_size": "14px",
        "font_weight": "600",
        "letter_spacing": "0.02em",
        "shadow": "0 0 20px rgba(16, 185, 129, 0.2)",
        "hover": {
          "background": "#34D399",
          "shadow": "0 0 30px rgba(16, 185, 129, 0.3)",
          "transform": "translateY(-1px)"
        },
        "active": {
          "transform": "scale(0.98)",
          "shadow": "0 0 10px rgba(16, 185, 129, 0.2)"
        }
      },
      "secondary": {
        "background": "transparent",
        "color": "#A1A1AA",
        "border": "1px solid rgba(63, 63, 70, 0.5)",
        "border_radius": "16px",
        "padding": "16px 24px",
        "hover": {
          "background": "rgba(255, 255, 255, 0.03)",
          "color": "#FAFAFA",
          "border_color": "rgba(255, 255, 255, 0.1)"
        }
      },
      "ghost": {
        "background": "transparent",
        "color": "#71717A",
        "hover": {
          "color": "#FAFAFA",
          "background": "rgba(255, 255, 255, 0.03)"
        }
      }
    },
    "input": {
      "background": "rgba(9, 9, 11, 0.6)",
      "border": "1px solid rgba(63, 63, 70, 0.4)",
      "border_radius": "12px",
      "padding": "14px 18px",
      "color": "#FAFAFA",
      "placeholder_color": "#52525B",
      "focus": {
        "border_color": "#10B981",
        "ring": "0 0 0 3px rgba(16, 185, 129, 0.1)",
        "background": "rgba(9, 9, 11, 0.8)"
      }
    },
    "card": {
      "background": "rgba(24, 24, 27, 0.6)",
      "border": "1px solid rgba(63, 63, 70, 0.4)",
      "border_radius": "24px",
      "padding": "24px",
      "backdrop_filter": "blur(20px)",
      "shadow": "0 25px 50px -12px rgba(0, 0, 0, 0.5)",
      "hover": {
        "border_color": "rgba(63, 63, 70, 0.6)",
        "shadow": "0 25px 50px -12px rgba(0, 0, 0, 0.6)"
      }
    },
    "navigation": {
      "background": "rgba(10, 10, 11, 0.8)",
      "border_bottom": "1px solid rgba(63, 63, 70, 0.3)",
      "backdrop_filter": "blur(20px)",
      "height": "64px",
      "item": {
        "color": "#71717A",
        "hover_color": "#FAFAFA",
        "active_color": "#10B981",
        "font_size": "14px",
        "font_weight": "500"
      }
    },
    "badge": {
      "background": "rgba(16, 185, 129, 0.1)",
      "color": "#10B981",
      "border": "1px solid rgba(16, 185, 129, 0.2)",
      "border_radius": "9999px",
      "padding": "4px 12px",
      "font_size": "11px",
      "font_weight": "600",
      "letter_spacing": "0.05em",
      "text_transform": "uppercase"
    }
  },

  "layout": {
    "container": {
      "max_width": "1280px",
      "padding": {
        "mobile": "16px",
        "tablet": "24px",
        "desktop": "32px"
      }
    },
    "grid": {
      "gap": "24px",
      "columns": {
        "mobile": 1,
        "tablet": 2,
        "desktop": 3,
        "wide": 4
      }
    },
    "breakpoints": {
      "sm": "640px",
      "md": "768px",
      "lg": "1024px",
      "xl": "1280px",
      "2xl": "1536px"
    }
  },

  "patterns": {
    "loading": {
      "skeleton": {
        "background": "linear-gradient(90deg, rgba(255,255,255,0.03) 25%, rgba(255,255,255,0.06) 50%, rgba(255,255,255,0.03) 75%)",
        "background_size": "200% 100%",
        "animation": "shimmer 1.5s infinite"
      },
      "spinner": {
        "size": "24px",
        "border": "2px solid rgba(255,255,255,0.1)",
        "border_top": "2px solid #10B981",
        "border_radius": "50%",
        "animation": "spin 1s linear infinite"
      }
    },
    "empty_state": {
      "icon_color": "#3F3F46",
      "text_color": "#71717A",
      "icon_size": "48px"
    },
    "divider": {
      "color": "rgba(63, 63, 70, 0.4)",
      "style": "solid",
      "width": "1px"
    }
  },

  "accessibility": {
    "focus_ring": {
      "width": "2px",
      "color": "#10B981",
      "offset": "2px"
    },
    "minimum_contrast": "4.5:1",
    "reduced_motion": "respects prefers-reduced-motion",
    "touch_target": {
      "minimum_size": "44px",
      "spacing": "8px"
    }
  }
}